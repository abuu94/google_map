SQL QUERY:
CREATE TABLE `home_leafletdb`.`worlddata` 
( `id` INT(11) NULL , `name` VARCHAR(25) NOT NULL , `latitude` DECIMAL(10) NOT NULL , `longitude` DECIMAL(10) NOT NULL ,
PRIMARY KEY (`id`)) ENGINE = InnoDB;

